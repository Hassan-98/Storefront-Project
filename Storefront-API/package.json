{
  "name": "storefront-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile": "tsc",
    "format": "prettier --config .prettierrc \"**/*.{ts,tsx}\" --write",
    "lint": "eslint",
    "migrate:up": "db-migrate up",
    "migrate:down": "db-migrate reset",
    "clean": "rm -rf dist/ || true",
    "archive": "zip -r Archive.zip . -x src/**\\*",
    "build": "npm run lint && npm run format && npm run compile",
    "postbuild": "npm run archive",
    "dev": "nodemon src/index.ts --watch \"src/**/*.{ts,tsx}\" -e ts,tsx",
    "start": "node dist/index.js",
    "pretest": "npm run clean && npm run compile && db-migrate --env testing reset && db-migrate --env testing up ",
    "test": "cross-env ENV=testing jasmine",
    "posttest": "db-migrate --env testing reset"
  },
  "keywords": ["Node.js", "Express", "PostgreSQL"],
  "author": "Hassan Ali",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "helmet": "^5.1.1",
    "jasmine": "^4.3.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/db-migrate-pg": "0.0.10",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.2",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "prettier": "^2.7.1",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
